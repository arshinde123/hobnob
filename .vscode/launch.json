            "type": "node",
            "request": "launch",
            "name": "Babel Node",
            "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/babel-node",
            "runtimeArgs": [
                "--presets",
                "@babel/env"
              ],
            "program": "${file}",
            "protocol": "inspector",
            "env": {
                "BABEL_ENV": "debug",
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Node with Experimental Modules",
